{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="sf-toolbar-info-with-next-pointer">
            <img alt="PhpMetrics" src="data:image/png;base64,
        " alt=""/>
            <span class="sf-toolbar-label">Complexity:</span>
            <span class="sf-toolbar-value">{{ collector.average.relativeSystemComplexity | round(2) }} </span>
        </span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Classes</b>
            <span>{{ collector.sum.nbClasses }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Interfaces</b>
            <span>{{ collector.sum.nbInterfaces }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Lack of cohesion of methods</b>
            <span class="sf-toolbar-status sf-toolbar-status-{% if collector.average.lcom > 0.98 and collector.average.lcom < 1.02 %}green{% elseif collector.average.lcom > 1.02 %}red{% else %}yellow{% endif %}">{{ collector.average.lcom | round(2) }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Average bugs by class</b>
            <span>{{ collector.average.bugs }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Average Difficulty</b>
            <span>{{ collector.average.difficulty | round(2) }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Average Weighted method count by class</b>
            <span>{{ collector.average.wmc | round(2) }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Average Relative system complexity</b>
            <span>{{ collector.average.relativeSystemComplexity | round(2) }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Average Cyclomatic complexity by class</b>
            <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.average.ccn | round(2) }}</span>
        </div>
    {% endset %}
    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}
{% endblock %}

{% block head %}
    {{ parent() }}
    {{ include('Collector/tinysort.html.twig') }}
{% endblock %}

{% block panel %}
    <h2>PhpMetrics Overview</h2>
    <div class="metrics">
        <div class="metric">
            <span class="value">{{ collector.average.lcom | round(2) }}</span>
            <span class="label">LCOM</span>
        </div>
        <div class="metric">
            <span class="value">{{ collector.average.ccn | round(2) }}</span>
            <span class="label">Average Cyclomatic complexity by class</span>
        </div>
        <div class="metric">
            <span class="value">{{ collector.average.wmc | round(2) }}</span>
            <span class="label">Average weighted method count by class</span>
        </div>
        <div class="metric">
            <span class="value">{{ collector.average.relativeSystemComplexity | round(2) }}</span>
            <span class="label">Average Relative system complexity</span>
        </div>
        <div class="metric">
            <span class="value">{{ collector.average.bugs | round(2) }}</span>
            <span class="label">Average bugs by class</span>
        </div>
        <div class="metric">
            <span class="value">{{ collector.final }}</span>
            <span class="label">Final classes</span>
        </div>
    </div>

    <table id="phpmetrics-table">
        <thead>
        <tr>
            <th>File</th>
            <th>Lines of code</th>
            <th>Cyclomatic complexity</th>
            <th>Bugs</th>
            <th>Difficulty</th>
        </tr>
        </thead>
        <tbody>
        {% for metric in collector.getClassMetrics %}
            <tr>
                <td>{{ metric.name }}</td>
                <td>{{ metric.get('lloc') }}</td>
                <td>{{ metric.get('ccn') }}</td>
                <td>{{ metric.get('bugs') }}</td>
                <td>{{ metric.get('difficulty') }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


    <script type="text/javascript">
        function initPhpMetrics() {
            var table = document.getElementById('phpmetrics-table')
                , tableHead = table.querySelector('thead')
                , tableHeaders = tableHead.querySelectorAll('th')
                , tableBody = table.querySelector('tbody')
            ;
            tableHead.addEventListener('click', function (e) {
                var tableHeader = e.target
                    , textContent = tableHeader.textContent
                    , tableHeaderIndex, isAscending, order
                ;
                if (textContent !== 'add row') {
                    while (tableHeader.nodeName !== 'TH') {
                        tableHeader = tableHeader.parentNode;
                    }
                    tableHeaderIndex = Array.prototype.indexOf.call(tableHeaders, tableHeader);
                    isAscending = tableHeader.getAttribute('data-order') === 'asc';
                    order = isAscending ? 'desc' : 'asc';
                    tableHeader.setAttribute('data-order', order);
                    tinysort(
                        tableBody.querySelectorAll('tr')
                        , {
                            selector: 'td:nth-child(' + (tableHeaderIndex + 1) + ')'
                            , order: order
                        }
                    );
                }
            });
        }

        document.addEventListener('DOMContentLoaded', initPhpMetrics, false);
        window.addEventListener('load', initPhpMetrics, false);
    </script>
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="PhpMetrics"></span>
        <strong>PhpMetrics</strong>
    </span>
{% endblock %}